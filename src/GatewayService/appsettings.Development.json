{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
  },
  "IdentityService": {
    "Url": "http://localhost:5000"
  },
  "ReverseProxy": {
    "Routes": {
      "ratingsRead":{
        "ClusterId": "ratings",
        "Match": {
          "Path": "/ratings/{**catch-all}",
          "Methods": ["GET"]
        },
        "Transforms":[
          {
            "PathPattern": "api/flagging/{**catch-all}"
          }
        ]
      },
      "ratingsWrite":{
        "ClusterId": "ratings",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/ratings/{**catch-all}",
          "Methods": ["POST", "PUT", "DELETE"]
        },
        "Transforms":[
          {
            "PathPattern": "api/flagging/{**catch-all}"
          }
        ]
      },
      "search":{
        "ClusterId": "search",
        "Match": {
          "Path": "/search/{**catch-all}",
          "Methods": ["GET"]
        },
        "Transforms":[
          {
            "PathPattern": "api/search/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "ratings": {
        "Destinations": {
          "ratingApi": {
            "Address": "http://localhost:7001"
          }
        }
      },
      "search": {
        "Destinations": {
          "searchApi": {
            "Address": "http://localhost:7002"
          }
        }
      }
    }
  }    
  }
}
